{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "reporter": ["lcov", "text-summary"],
  "check-coverage": true,
  "report-dir": "./coverage",
  "include": ["**/*.ts"],
  "exclude": ["**/*.spec.ts", "**/*.spec.js", "**/*.integration.ts", "**/*.integration.js"],
  "branches": 70,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
